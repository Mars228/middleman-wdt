<!-- header --> 
<header id="header" class="header">  

	<div class="container">

		<h1 class="logo pull-left">
			<a class="scrollto" href="#welcome">
				<span class="logo-title"><%= data.site.title %></span>
			</a>
		</h1>

		<nav id="main-nav" class="main-nav navbar-right" role="navigation">

			<div class="navbar-header">
				<button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-collapse">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button><!--//nav-toggle-->
			</div> 

			<div class="navbar-collapse collapse" id="navbar-collapse">
				<ul class="nav navbar-nav">

					<% data.nav.menu.each do |m| %>
					<li class="nav-item"><%= link_to m[:menuItem], m[:menuLink], :class => "scrollto" %></li>
					<% end %>
					<li class="nav-item"><a href="https://middlemanapp.com/basics/install/">Docs</a></li>
				</ul>
			</div>

		</nav>

	</div>

</header>
<!--//header-->